SHARELATEX_EMAIL_FROM_ADDRESS=example@mail.com
SHARELATEX_EMAIL_SMTP_HOST=smtp.exmail.qq.com
SHARELATEX_EMAIL_SMTP_PORT=465
SHARELATEX_EMAIL_SMTP_SECURE=true
SHARELATEX_EMAIL_SMTP_USER=example@mail.com
SHARELATEX_EMAIL_SMTP_PASS=
SHARELATEX_EMAIL_SMTP_NAME=example@mail.com

SHARELATEX_EMAIL_SMTP_TLS_REJECT_UNAUTH=true
SHARELATEX_EMAIL_SMTP_IGNORE_TLS=true
SHARELATEX_CUSTOM_EMAIL_FOOTER=

