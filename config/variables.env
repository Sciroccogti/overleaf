SHARELATEX_APP_NAME=USTC Latex

#CASURL=https://passport.ustc.edu.cn/

DOCKER_RUNNER=true
TEXLIVE_IMAGE=texlive-full
TEXLIVE_IMAGE_USER=root
TEX_LIVE_DOCKER_IMAGE=texlive-full
ALL_TEX_LIVE_DOCKER_IMAGES=
ALLOWED_IMAGES=

SANDBOXED_COMPILES=true
COMPILER_PATH='/usr/local/texlive/2022/bin/x86_64-linux:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin'
SANDBOXED_COMPILES_SIBLING_CONTAINERS=true
COMPILES_HOST_DIR=/srv/toolkit/data/sharelatex/data/compiles
SANDBOXED_COMPILES_HOST_DIR=/srv/toolkit/data/sharelatex/data/compiles

ENABLED_LINKED_FILE_TYPES=project_file,project_output_file

# Enables Thumbnail generation using ImageMagick
ENABLE_CONVERSIONS=true

# Disables email confirmation requirement
EMAIL_CONFIRMATION_DISABLED=true

# temporary fix for LuaLaTex compiles
# see https://github.com/overleaf/overleaf/issues/695
TEXMFVAR=/var/lib/sharelatex/tmp/texmf-var

## Nginx
# NGINX_WORKER_PROCESSES=4
# NGINX_WORKER_CONNECTIONS=768

## Set for TLS via nginx-proxy
SHARELATEX_BEHIND_PROXY=true
SHARELATEX_SECURE_COOKIE=true

SHARELATEX_SITE_URL=

SHARELATEX_NAV_TITLE=USTC Latex

# SHARELATEX_HEADER_IMAGE_URL=http://somewhere.com/mylogo.png

SHARELATEX_ADMIN_EMAIL=example@mail.com

